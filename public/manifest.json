{
  "manifest_version": 3,
  "name": "Mindful",
  "description": "Store lists of bookmarks in separate categories",
  "version": "1.0.0",
  "key": "",

  "chrome_url_overrides": {
    "newtab": "NewTab.html"
  },

  "options_ui": {
    "page": "ManageAccount.html",
    "open_in_tab": true
  },

  "icons": {
    "16": "assets/icon-16.png",
    "32": "assets/icon-32.png",
    "48": "assets/icon-48.png",
    "128": "assets/icon-128.png"
  },

  "permissions": [
    "storage", 
    "tabs"
  ],

  "optional_permissions": [
    "bookmarks"
  ],

  "optional_host_permissions": [
    "https://2rra98zl35.execute-api.us-west-1.amazonaws.com/*",
    "<all_urls>"
  ],

  "action": {
    "default_popup": "PopUp.html",
    "default_icon": {
      "16": "assets/icon-16.png",
      "32": "assets/icon-32.png",
      "48": "assets/icon-48.png",
      "128": "assets/icon-128.png"
    }
  }
}