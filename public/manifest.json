{
  "manifest_version": 3,
  "name": "Mindful",
  "description": "Store lists of bookmarks in separate categories",
  "version": "1.0.0",

  "chrome_url_overrides": {
    "newtab": "NewTab.html"
  },

  "icons": {
    "16": "assets/icon-16.png",
    "32": "assets/icon-32.png",
    "48": "assets/icon-48.png",
    "128": "assets/icon-128.png"
  },

  "permissions": [
    "storage", 
    "tabs",
    "windows"
  ],

  "action": {
    "default_popup": "Popup.html",
    "default_icon": {
      "16": "assets/icon-16.png",
      "32": "assets/icon-32.png",
      "48": "assets/icon-48.png",
      "128": "assets/icon-128.png"
    }
  }
}